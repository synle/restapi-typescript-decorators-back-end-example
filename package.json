{
  "name": "restapi-typescript-decorators-example",
  "version": "2.0.1",
  "description": "Example repo that uses the restapi-typescript-decorators",
  "main": "build/index",
  "scripts": {
    "start": "node mock-server.js",
    "build": "rm -rf build/* && tsc",
    "format": "npx prettier --config ./.prettierrc --write **/**/**/**/*.{ts,tsx,js,jsx,scss} *.{json,MD}",
    "test": "npx ts-node src/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synle/restapi-typescript-decorators-example.git"
  },
  "keywords": [
    "restapi",
    "fetch",
    "decorator",
    "annotation"
  ],
  "author": "Sy Le",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/synle/restapi-typescript-decorators-example/issues"
  },
  "homepage": "https://github.com/synle/restapi-typescript-decorators-example#readme",
  "dependencies": {
    "restify": "^8.5.1",
    "restapi-typescript-decorators": "^7"
  },
  "devDependencies": {
    "typescript": "^4"
  },
  "peerDependencies": {}
}
